<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:cloudm="http://schemas.android.com/apk/res-auto"
    xmlns:check="http://schemas.android.com/apk/res-auto"
    xmlns:detect="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/public_bg"
    android:orientation="vertical" >

    
<FrameLayout 
    android:layout_width="match_parent"
        android:layout_height="match_parent"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
        <com.cloudmachine.autolayout.widgets.TitleView
        android:id="@+id/title_layout"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
        />
        

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/rl_scanning"
            android:layout_width="match_parent"
            android:layout_height="@dimen/detect_now_height"
            android:background="@color/public_title_color" >

            <ImageView
                android:id="@+id/iv_oval_detecting"
                android:layout_width="@dimen/detecting_oval_size"
                android:layout_height="@dimen/detecting_oval_size"
                android:layout_centerInParent="true"
                android:src="@drawable/oval_detecting" />

            <ImageView
                android:id="@+id/iv_oval_detect"
                android:layout_width="@dimen/detect_oval_size"
                android:layout_height="@dimen/detect_oval_size"
                android:layout_centerInParent="true"
                android:src="@drawable/oval_detect" />

            <ImageView
                android:layout_width="@dimen/oval_size"
                android:layout_height="@dimen/oval_size"
                android:layout_centerInParent="true"
                android:src="@drawable/oval" />

            <Button
                        android:id="@+id/btn_detect_now"
                        android:layout_width="@dimen/btn_detect_now_width"
                        android:layout_height="@dimen/btn_detect_now_height"
                        android:layout_centerInParent="true"
                        android:background="@drawable/one_key_detection"
                        android:text="@string/detect_now"
                        android:textSize="@dimen/public_textsize1" />
            

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rl_check_report"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:id="@+id/ll_basic_info"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical"
                android:paddingBottom="1dp"
                android:visibility="visible" >

                <LinearLayout
                    android:id="@+id/test_report"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/detect_report_height"
                    android:orientation="horizontal" 
                    android:background="@drawable/bg_list_item" 
                    >

                    <ImageView
                        android:layout_width="@dimen/detect_report_iconsize"
                        android:layout_height="@dimen/detect_report_iconsize"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin"
                        android:src="@drawable/detect_report" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin" >

                        <TextView
                            android:id="@+id/tv_test_report"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/detect_report"
                            android:textColor="@color/public_color_black"
                            android:textSize="@dimen/public_textsize1" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_test_report"
                            android:layout_marginTop="@dimen/public_text_margintop"
                            android:text="@string/get_newdata"
                            android:textColor="@color/public_text_grey1"
                            android:textSize="@dimen/public_textsize2" />

                        <ImageView
                            android:layout_width="@dimen/arrow_next_width"
                            android:layout_height="@dimen/arrow_next_height"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/public_title_margin"
                            android:src="@drawable/arrow_next" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/public_view_height1"
                    android:background="@color/public_view_color" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/public_layout_height2"
                    android:orientation="horizontal" >
                    <LinearLayout 
                        android:layout_width="match_parent"
                    	android:layout_height="match_parent"
                        >
                        <LinearLayout 
                            android:id="@+id/ailAmount_layout"
                            android:layout_width="match_parent"
                    	android:layout_height="match_parent"
                    	android:layout_weight="1"
                    	android:background="@drawable/bg_list_item" 
                            >
                            
                        </LinearLayout>
                        <LinearLayout 
                            android:id="@+id/workTime_layout"
                            android:layout_width="match_parent"
                    	android:layout_height="match_parent"
                    	android:layout_weight="1"
                    	android:background="@drawable/bg_list_item" 
                            >
                            
                        </LinearLayout>
                    </LinearLayout>
                    
                    
                    
                    
                    <RelativeLayout 
                        android:layout_width="match_parent"
                    	android:layout_height="@dimen/public_divider_vertical_height"
                    	android:layout_centerVertical="true"
                        >
                        <View
                        android:id="@+id/mid_line"
                        android:layout_width="@dimen/public_line_wh"
                        android:layout_height="match_parent"
                        android:background="@color/public_line" 
                        android:layout_marginLeft="@dimen/oil_marginRight"
                        android:layout_marginRight="@dimen/oil_marginRight"
                        android:layout_centerInParent="true"
                        />
                        
                        
                        <RelativeLayout 
                            android:layout_width="wrap_content"
                    		android:layout_height="match_parent"
                    		android:layout_toLeftOf="@id/mid_line"
                    		android:layout_centerVertical="true"
                            >
                            <TextView
                                android:id="@+id/o_text1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:text="剩余油位"
                            android:textColor="@color/public_color_black"
                            android:textSize="@dimen/public_textsize3" />
                            
                            <com.cloudmachine.autolayout.widgets.DynamicWave 
                            android:id="@+id/iv_residual_oil"
                            android:layout_width="@dimen/oil_level_size"
                            android:layout_height="@dimen/oil_level_size"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentTop="true"
                            android:layout_above="@id/o_text1"
                            />
                        </RelativeLayout>
                        
                        <RelativeLayout
                        
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_toRightOf="@id/mid_line"
                    		android:layout_centerVertical="true"
                         >

                            <TextView
                                android:id="@+id/o_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="今日工作时长"
                            android:textColor="@color/public_color_black"
                            android:textSize="@dimen/public_textsize3" 
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            />
                            <LinearLayout 
                                android:layout_width="wrap_content"
                            	android:layout_height="match_parent"
                            	android:layout_alignParentTop="true"
                            	android:layout_above="@id/o_text2"
                                >
                                <com.cloudmachine.utils.widgets.TextProgressBar
                            android:id="@+id/work_hours"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="@dimen/work_hour_width"
                            android:layout_height="@dimen/work_hour_height"
                            android:progressDrawable="@drawable/progress" 
                            android:layout_gravity="center_vertical"
                            />
                            </LinearLayout>
                        

                        
                    </RelativeLayout>
                        
                    </RelativeLayout>


                    

                    
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/public_view_height1"
                    android:background="@color/public_view_color" />

                <LinearLayout
                    android:id="@+id/repair_record"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/detect_report_height"
                    android:orientation="horizontal" 
                    android:background="@drawable/bg_list_item" 
                    >

                    <ImageView
                        android:layout_width="@dimen/detect_report_iconsize"
                        android:layout_height="@dimen/detect_report_iconsize"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin"
                        android:src="@drawable/inspection" />

                    <RelativeLayout
                        android:id="@+id/rl_repair_history"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin" >

                        <TextView
                            android:id="@+id/tv_repair_report"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/repair_record"
                            android:textColor="@color/public_color_black"
                            android:textSize="@dimen/public_textsize1" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_repair_report"
                            android:layout_marginTop="@dimen/public_text_margintop"
                            android:text="@string/history_repair_record"
                            android:textColor="@color/public_text_grey1"
                            android:textSize="@dimen/public_textsize2" />

                        <ImageView
                            android:layout_width="@dimen/arrow_next_width"
                            android:layout_height="@dimen/arrow_next_height"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/public_title_margin"
                            android:src="@drawable/arrow_next" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/list_divider_color2" />

                <LinearLayout
                    android:id="@+id/member_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/detect_report_height"
                    android:orientation="horizontal" 
                    android:background="@drawable/bg_list_item" 
                    >

                    <ImageView
                        android:layout_width="@dimen/detect_report_iconsize"
                        android:layout_height="@dimen/detect_report_iconsize"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin"
                        android:src="@drawable/conference" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/public_title_margin" >

                        <TextView
                            android:id="@+id/tv_Membership"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/conference"
                            android:textColor="@color/public_color_black"
                            android:textSize="@dimen/public_textsize1" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_Membership"
                            android:layout_marginTop="@dimen/public_text_margintop"
                            android:text="@string/share_conference"
                            android:textColor="@color/public_text_grey1"
                            android:textSize="@dimen/public_textsize2" />

                        <ImageView
                            android:layout_width="@dimen/arrow_next_width"
                            android:layout_height="@dimen/arrow_next_height"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/public_title_margin"
                            android:src="@drawable/arrow_next" />
                    </RelativeLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divider_height"
                    android:background="@color/list_divider_color2" />
                <LinearLayout 
                        android:id="@+id/map_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        >
                        <com.amap.api.maps2d.MapView
					        android:id="@+id/map"
					        android:layout_width="fill_parent"
					        android:layout_height="fill_parent" />
                    </LinearLayout>
            </LinearLayout>
            <!-- </ScrollView> -->


            <!-- 检测成功布局 -->

            <LinearLayout
                android:id="@+id/ll_detecting"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone" >

                <!-- 检测结果的头标题 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/detect_title_height"
                    android:background="@color/detect_title_color"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/detecting_marginleft1"
                        android:text="@string/detect_item"
                        android:textColor="@color/detect_title_text_color"
                        android:textSize="@dimen/public_textsize3" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/detecting_marginleft2"
                        android:text="@string/refer_range"
                        android:textColor="@color/detect_title_text_color"
                        android:textSize="@dimen/public_textsize3" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/detecting_marginleft3"
                        android:text="@string/detecting_result"
                        android:textColor="@color/detect_title_text_color"
                        android:textSize="@dimen/public_textsize3" />
                </LinearLayout>
				<RelativeLayout 
				    android:layout_width="match_parent"
                    android:layout_height="match_parent"
				    >
				    <com.cloudmachine.autolayout.widgets.RadiusButtonView
			        android:id="@+id/button_ok"
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/detecting_oval_button_height"
			        android:layout_marginLeft="@dimen/public_layout_padding_left"
			        android:layout_marginRight="@dimen/public_layout_padding_left"
			        android:layout_gravity="center_horizontal"
			        cloudm:rbtn_strokeColor="@color/public_blue_bg"
			        cloudm:rbtn_fillColor_nm="@color/public_blue_bg"
			        cloudm:rbtn_fillColor_dw="@color/public_b_20"
			        cloudm:rbtn_text="@string/mc_detecting_oval_button_text"
			        cloudm:rbtn_textColor="@color/public_white"
			        cloudm:rbtn_textSize="@dimen/about_button_textsize" 
			        android:layout_marginBottom="@dimen/main1_empty_image_to_text"
			        android:layout_alignParentBottom="true"
			        />
				    <ListView
                    android:id="@+id/xlv_check_report"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:drawSelectorOnTop="false"
                    android:fadingEdgeLength="0.0dp"
                    android:layoutAnimation="@anim/navagation_gridview_anim"
                    android:visibility="visible" 
                    android:layout_alignParentTop="true"
                    android:layout_above="@id/button_ok"
                    android:divider="@null"
                    />
				</RelativeLayout>
                
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/no_test_data_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/commom_transparent_bg"
        android:gravity="center_horizontal"
        android:visibility="gone"
        >
        <LinearLayout 
            android:layout_width="@dimen/mc_main_no_test_layout_width"
        	android:layout_height="@dimen/mc_main_no_test_layout_height"
        	android:layout_marginTop="@dimen/mc_main_no_test_layout_marginTop"
        	android:background="@drawable/radius_button_white"
        	android:orientation="vertical"
            >
            <ImageView 
                android:layout_width="@dimen/mc_main_no_test_layout_width"
        		android:layout_height="@dimen/mc_main_no_test_iamge_height"
                android:background="@drawable/mc_main_no_test"
                />
            <TextView 
                style="@style/mc_main_no_data_text"
        		android:text="@string/mc_no_data1"
        		android:layout_marginTop="@dimen/mc_main_no_test_iamge_to_text"
                />
            <TextView 
                style="@style/mc_main_no_data_text"
        		android:text="@string/mc_no_data2"
        		android:layout_marginTop="@dimen/mc_main_no_test_text_to_text"
                />
            <com.cloudmachine.autolayout.widgets.RadiusButtonView
			        android:id="@+id/iKnow_button"
			        android:layout_width="@dimen/mc_main_no_test_button_width"
			        android:layout_height="@dimen/mc_main_no_test_button_height"
			        android:layout_gravity="center_horizontal"
			        cloudm:rbtn_strokeColor="@color/public_green"
			        cloudm:rbtn_fillColor_nm="@color/public_green"
			        cloudm:rbtn_fillColor_dw="@color/public_b_20"
			        cloudm:rbtn_text="@string/mc_no_data_button"
			        cloudm:rbtn_textColor="@color/public_white"
			        cloudm:rbtn_textSize="@dimen/about_button_textsize" 
			        android:layout_marginTop="@dimen/mc_main_no_test_text_to_button"
			        />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
<LinearLayout 
    	android:id="@+id/mc_main_guide_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/public_transparent_50"
        android:orientation="vertical"
        android:visibility="gone"
    >
    <ImageView 
        android:id="@+id/mc_main_guide_image"
        android:layout_width="@dimen/mc_main_guide_image_width"
        android:layout_height="@dimen/mc_main_guide_image_height"
        android:layout_marginTop="@dimen/mc_main_guide_image_marginTop"
        android:layout_marginLeft="@dimen/mc_main_guide_image_marginLeft"
        android:background="@drawable/mc_main_guide_image"
        
        />
</LinearLayout>
</RelativeLayout>