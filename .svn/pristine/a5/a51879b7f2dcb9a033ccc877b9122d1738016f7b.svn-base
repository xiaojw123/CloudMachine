apply plugin: 'com.android.application'

android {
    compileSdkVersion 24
    buildToolsVersion "24.0.3"
    useLibrary 'org.apache.http.legacy'
    defaultConfig {
        applicationId "com.cloudmachine"
        minSdkVersion 16
        targetSdkVersion 22
        versionCode 87
        versionName "2.1.2"
        testInstrumentationRunner "android.support.test.runner.AndroidJUnitRunner"
        multiDexEnabled true
    }
    lintOptions{
        checkReleaseBuilds false
        abortOnError false
    }
    buildTypes {
        release {
            minifyEnabled true
            proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.pro'
        }
    }
}

dependencies {
    compile fileTree(include: ['*.jar'], dir: 'libs')
    androidTestCompile('com.android.support.test.espresso:espresso-core:2.2.2', {
        exclude group: 'com.android.support', module: 'support-annotations'
    })
    compile 'com.android.support:appcompat-v7:24.2.1'
    testCompile 'junit:junit:4.12'
    compile files('libs/AMap_2DMap_V2.3.0.jar')
    compile files('libs/AMap_Location_V3.0.0_20160922.jar')
    compile files('libs/AMap_Services_V2.3.0.jar')
    compile files('libs/download-manager.jar')
    compile files('libs/gson-2.2.4.jar')
    compile files('libs/httpmime-4.1.2.jar')
    compile files('libs/jpush-android-2.2.0.jar')
    compile files('libs/pinyin4j-2.5.0.jar')
    compile files('libs/systembartint-1.0.4.jar')
    compile files('libs/uk_co_senab_photoview.jar')
    compile files('libs/umeng-analytics-v6.0.1.jar')
    compile files('libs/universal-image-loader-1.9.0.jar')
    compile files('libs/utdid4all-1.0.4.jar')
    compile 'com.alibaba.sdk.android.plugins:alisdk-hotfix:1.3.3'
    compile files('libs/baidumapapi_base_v3_6_0.jar')
    compile files('libs/baidumapapi_cloud_v3_6_0.jar')
    compile files('libs/baidumapapi_map_v3_6_0.jar')
    compile files('libs/baidumapapi_radar_v3_6_0.jar')
    compile files('libs/baidumapapi_search_v3_6_0.jar')
    compile files('libs/baidumapapi_util_v3_6_0.jar')
    compile files('libs/locSDK_6.12.jar')
    compile files('libs/libammsdk.jar')
    compile 'com.google.android.gms:play-services-appindexing:8.4.0'
    compile files('libs/alipaySingle-20161222.jar')
    compile 'com.github.rey5137:material:1.2.2'
}

repositories {
    maven {
        url "http://repo.baichuan-android.taobao.com/content/groups/public/"
    }
}