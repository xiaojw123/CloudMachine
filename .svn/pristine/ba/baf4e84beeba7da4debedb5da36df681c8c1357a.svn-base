<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <com.cloudmachine.autolayout.widgets.TitleView
        android:id="@+id/title_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        />

    <RelativeLayout
        android:id="@+id/phoneLayout"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/f11"
            style="@style/personal_left_text"
            android:text="账号"
            />

        <TextView
            android:id="@+id/edit_textPhone"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text=""
            />

        <ImageView
            android:id="@+id/iv_calendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/calendar"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="42px"
            />

    </RelativeLayout>
    <LinearLayout
        style="@style/line_layout"
        android:layout_marginLeft="@dimen/public_layout_padding_left"
        />

    <RelativeLayout
        android:id="@+id/rl_current_month"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/tv_current_month"
            style="@style/personal_left_text"
            android:text="本月"
            />

        <TextView
            android:id="@+id/tv_current_month_value"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text="统计说明"
            />


    </RelativeLayout>

    <LinearLayout
        style="@style/line_layout"
        android:layout_marginLeft="@dimen/public_layout_padding_left"
        />

    <RelativeLayout
        android:id="@+id/rl_work_days"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/tv_work_days"
            style="@style/personal_left_text"
            android:text="工作天数"
            />

        <TextView
            android:id="@+id/tv_work_days_value"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text="18小时"
            />
    </RelativeLayout>

    <LinearLayout
        style="@style/line_layout"
        android:layout_marginLeft="@dimen/public_layout_padding_left"
        />
    <RelativeLayout
        android:id="@+id/rl_total_hours"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/tv_total_hours"
            style="@style/personal_left_text"
            android:text="累积工时"
            />

        <TextView
            android:id="@+id/tv_total_hours_value"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text="90小时36分"
            />
    </RelativeLayout>
    <LinearLayout
        style="@style/line_layout"
        android:layout_marginLeft="@dimen/public_layout_padding_left"
        />
    <RelativeLayout
        android:id="@+id/rl_ave_hours"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/tv_ave_hours"
            style="@style/personal_left_text"
            android:text="平均工时"
            />

        <TextView
            android:id="@+id/tv_ave_hours_value"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text="5小时2分钟"
            />
    </RelativeLayout>
    <LinearLayout
        style="@style/line_layout"
        android:layout_marginLeft="@dimen/public_layout_padding_left"
        />
    <RelativeLayout
        android:id="@+id/rl_work_rate"
        style="@style/layout_item"
        >

        <TextView
            android:id="@+id/tv_work_rate"
            style="@style/personal_left_text"
            android:text="开工率"
            />

        <TextView
            android:id="@+id/tv_work_rate_value"
            style="@style/personal_edit_text"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/public_layout_padding_left"
            android:background="@null"
            android:text="20%"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_check_report"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
</LinearLayout>