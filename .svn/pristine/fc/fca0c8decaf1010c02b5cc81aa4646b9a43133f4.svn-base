<?xml version="1.0" encoding="utf-8"?>
<com.cloudm.autolayout.AutoRelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="180px"
  android:background="@color/white"
  >
  <LinearLayout
                    style="@style/line_layout"
                    android:layout_marginLeft="18px"
                    />
  <LinearLayout 
      android:layout_width="match_parent"
  	android:layout_height="match_parent"
      >
      <RelativeLayout 
          android:layout_width="138px"
  		   android:layout_height="match_parent"
          >
          <LinearLayout 
              android:layout_width="wrap_content"
  				android:layout_height="match_parent"
  				android:orientation="vertical"
  				android:layout_centerHorizontal="true"
              >
              <TextView 
                  android:id="@+id/text_line1"
                  android:layout_width="@dimen/public_line_wh"
  					android:layout_height="match_parent"
  					android:layout_weight="1"
  					android:background="@color/public_line"
                  />
              <TextView 
                  android:id="@+id/text_line2"
                  android:layout_width="@dimen/public_line_wh"
  					android:layout_height="match_parent"
  					android:layout_weight="1"
  					android:background="@color/public_line"
                  />
          </LinearLayout>
          <ImageView 
              android:id="@+id/left_iamge"
              android:layout_width="wrap_content"
  				android:layout_height="wrap_content"
  				android:layout_centerInParent="true"
  				android:background="@drawable/schedual_circle_select"
              />
      </RelativeLayout>
      <RelativeLayout 
          android:layout_width="match_parent"
  				android:layout_height="match_parent"
          >
          <LinearLayout 
              android:layout_width="match_parent"
  				android:layout_height="wrap_content"
  				android:layout_centerVertical="true"
  				android:orientation="vertical"
  				android:paddingTop="38px"
  				android:paddingBottom="38px"
              >
              <TextView 
                  android:id="@+id/text1"
                  android:layout_width="wrap_content"
  				  android:layout_height="wrap_content"
  				  android:textSize="@dimen/public_textsize3"
  				  android:singleLine="true"
  				  android:paddingBottom="20px"
                  />
              <TextView 
                  android:id="@+id/text2"
                  android:layout_width="wrap_content"
  				  android:layout_height="wrap_content"
  				  android:textSize="@dimen/public_textsize3"
  				  android:singleLine="true"
                  />
          </LinearLayout>
          
      </RelativeLayout>
  </LinearLayout>
  
    	
</com.cloudm.autolayout.AutoRelativeLayout>
