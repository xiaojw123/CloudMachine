<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mc_main_bg"
    android:orientation="vertical" >

    <com.cloudmachine.utils.widgets.TitleView
        android:id="@+id/title_layout"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
        >
    </com.cloudmachine.utils.widgets.TitleView>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="@dimen/historical_track_time_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/common_left_gap"
        >
        <LinearLayout 
            android:id="@+id/call_phone_layout"
            android:layout_width="@dimen/historical_track_time_height"
        	android:layout_height="match_parent"
        	android:gravity="center"
        	android:layout_alignParentRight="true"
            >
            <ImageView 
                android:id="@+id/call_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_btn_call_phone"
                />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_toLeftOf="@id/call_phone_layout"
            >
            <LinearLayout 
                android:layout_width="match_parent"
            	android:layout_height="match_parent"
            	android:layout_weight="1"
            	android:gravity="center_vertical"
                >
                <TextView 
                    android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:text="网点"
            		style="@style/agent_text1"
                    />
                <TextView 
                    android:id="@+id/agent_name"
                    android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:singleLine="true"
            		style="@style/agent_text1"
            		android:layout_marginLeft="@dimen/common_left_gap"
                    />
            </LinearLayout>
            <TextView 
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/common_line_height"
                        android:background="@color/common_line"
                        />
            <LinearLayout 
                android:layout_width="match_parent"
            	android:layout_height="match_parent"
            	android:layout_weight="1"
            	android:gravity="center_vertical"
                >
                <TextView 
                    android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:text="安装"
            		style="@style/agent_text1"
                    />
                <TextView 
                    android:id="@+id/agent_user_name"
                    android:layout_width="wrap_content"
            		android:layout_height="wrap_content"
            		android:singleLine="true"
            		style="@style/agent_text1"
            		android:layout_marginLeft="@dimen/common_left_gap"
                    />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
	<TextView 
	    android:id="@+id/rrTitle"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_mid_title_height"
        android:layout_marginLeft="@dimen/layout_item_padding_left"
        android:text="@string/agent_info_t_1"
        android:textColor="@color/mc_device_old_text"
        android:textSize="@dimen/common_text2"
        android:gravity="center_vertical"
	    />
	<ListView
		        android:id="@+id/agent_listview"
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:dividerHeight="0dp"
		        android:divider="@color/line_bg"
		        android:scrollbars="none"
		        
		        />
</LinearLayout>