<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item
        >
        <shape
            android:shape="rectangle"
            >
            <corners android:radius="@dimen/dimen_size_2" />
            <gradient
                android:angle="90"
                android:startColor="#12b2b2b2"
                android:centerColor="#03b2b2b2"
                android:endColor="#00b2b2b2"
                />
        </shape>

    </item>

    <item
        android:left="@dimen/dimen_size_2"
        android:right="@dimen/dimen_size_2"
        android:top="@dimen/dimen_size_2"
        android:bottom="@dimen/dimen_size_2"
        >
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_size_2" />

            <solid
                android:color="@color/white"
                />
        </shape>
    </item>

</layer-list>